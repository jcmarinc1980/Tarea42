        <p class=\"resultado\">{{ resultado }}</p>\r\n        </div>\r\n        <button @click=\"regresar\" class=\"regresar-btn\">Regresar</button>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        inputValue: null,\r\n        unidadOrigen: 'celsius',\r\n        unidadDestino: 'celsius',\r\n        resultado: null\r\n      };\r\n    },\r\n    methods: {\r\n      convertir() {\r\n        const unidades = {\r\n          celsius: {\r\n            celsius: valor => valor,\r\n            fahrenheit: valor => (valor * 9/5) + 32,\r\n            kelvin: valor => valor + 273.15\r\n          },\r\n          fahrenheit: {\r\n            celsius: valor => (valor - 32) * 5/9,\r\n            fahrenheit: valor => valor,\r\n            kelvin: valor => (valor + 459.67) * 5/9\r\n          },\r\n          kelvin: {\r\n            celsius: valor => valor - 273.15,\r\n            fahrenheit: valor => (valor * 9/5) - 459.67,\r\n            kelvin: valor => valor\r\n          }\r\n        };\r\n  \r\n        const valor = parseFloat(this.inputValue);\r\n        const conversion = unidades[this.unidadOrigen][this.unidadDestino];\r\n  \r\n        if (!isNaN(valor) && conversion) {\r\n          this.resultado = conversion(valor).toFixed(2) + ' ' + this.unidadDestino.toUpperCase();\r\n        } else {\r\n          this.resultado = 'Entrada inválida';\r\n        }\r\n      },\r\n      regresar() {\r\n        this.$router.push('/');\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Estilos */\r\n  .conversion-container {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    height: 100vh;\r\n  }\r\n  \r\n  .conversion-box {\r\n    max-width: 400px;\r\n    padding: 20px;\r\n    border: 3px solid black;\r\n    border-radius: 10px;\r\n  }\r\n  \r\n  .input-group {\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  .output-group {\r\n    text-align: center;\r\n  }\r\n  \r\n  .resultado {\r\n    font-size: 24px;\r\n    color: red;\r\n  }\r\n  \r\n  .regresar-btn {\r\n    display: block;\r\n    margin: 20px auto 0;\r\n    padding: 10px 20px;\r\n    background-color: #333;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    text-align: center;\r\n  }\r\n  </style>\r\n  "]}]}